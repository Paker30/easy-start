{
  "extends": "airbnb-base",
  "env": {
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {}
  },
  "rules": {
    "semi": 2,
    "max-len": ["error", { "code": 120 }],
    "object-curly-newline": [
      "error",
      { "ObjectPattern": { "multiline": true } }
    ]
  }
}
